{
  "manifest_version": 3,
  "name": "YouTube SPEEEEEEEEEED",
  "version": "0.1.0",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "run_at": "document_idle",
      "js": ["script.js"],
      "css": ["style.css"]
    }
  ],
  "permissions": ["activeTab", "contextMenus", "storage", "scripting"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html",
    "default_title": "__MSG_name__",
    "all_frames": true
  }
}
